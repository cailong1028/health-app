<div class="container finger-register">
    <div class="row">
        <div class="col-lg-12 col-md-12">
            <div class="panel panel-content">
                <div class="panel-heading">
                    <h3>{{gettext 'Finger register'}}</h3>
                </div>
                <div class="panel-body">
                    <div class="pull-right">
                        <a href="javascript: window.history.back();" class="btn btn-default">{{gettext "Back"}}</a>
                    </div>
                    <div class="tab-content">
                        <div class="tab-pane fade in active" role="tabpanel">
                            <img src="{{assets '/images/finger.jpg'}}" class="img-rounded"/>
                            <label id="finger-register-info" style="color: red; font-weight: bold;">{{gettext "please press finger"}}</label>
                        </div>
                    </div>
                </div>{{! panel-body end}}
            </div>{{! panel end}}
        </div>
        </div>
    </div>
</div>
